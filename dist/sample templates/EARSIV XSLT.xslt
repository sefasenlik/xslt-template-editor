      <xsl:text>TL</xsl:text>
    </xsl:if>
    <xsl:if test="n1:Invoice/cbc:DocumentCurrencyCode != 'TRL' and n1:Invoice/cbc:DocumentCurrencyCode != 'TRY'">
      <xsl:value-of select="n1:Invoice/cbc:DocumentCurrencyCode"/>
    </xsl:if>
  </xsl:variable>
  <xsl:template match="/">
